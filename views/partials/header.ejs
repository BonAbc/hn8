    <!-- Structured Data -->
    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@type": "AccountingService",
        "name": "Hieu CPA",
        "description": "CPA, SAP, and EA professional services",
        "url": "https://hieuncpa.com",
        "address": {
          "@type": "PostalAddress",
          "addressLocality": "Franklin Park",
          "addressRegion": "IL",
          "addressCountry": "US"
        },
        "areaServed": [
          { "@type": "Place", "name": "Franklin Park, IL" },
          { "@type": "Place", "name": "Niles, IL" },
          { "@type": "Place", "name": "Carol Stream, IL" },
          { "@type": "Place", "name": "Chicago, IL" },
          { "@type": "Place", "name": "Naperville, IL" },
          { "@type": "Country", "name": "United States" },
          { "@type": "Country", "name": "Worldwide" }
        ],
        "contactPoint": {
          "@type": "ContactPoint",
          "telephone": "+1-847-208-9053",
          "email": "hn@hieuncpa.com",
          "contactType": "customer service"
        }
      }
    </script>

